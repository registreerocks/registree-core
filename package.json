{
  "name": "registree-core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch | pino-pretty -c -l",
    "start:debug": "nest start --debug --watch | pino-pretty -c -l",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.11",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.11",
    "@nestjs/graphql": "^7.9.8",
    "@nestjs/mongoose": "^7.2.3",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.11",
    "@nestjs/terminus": "^7.1.0",
    "apollo-server-core": "^2.19.2",
    "apollo-server-express": "^2.19.2",
    "aws-sdk": "^2.770.0",
    "axios": "^0.20.0",
    "class-transformer": "patch:class-transformer@^0.3.1#./skip-promises-class-transformer.diff",
    "class-validator": "^0.13.1",
    "dataloader": "^2.0.0",
    "date-fns": "^2.16.1",
    "graphql": "^15.3.0",
    "graphql-middleware": "^6.0.3",
    "graphql-relay": "^0.6.0",
    "graphql-shield": "^7.5.0",
    "graphql-upload": "^11.0.0",
    "hasha": "^5.2.2",
    "jwks-rsa": "^1.10.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.15",
    "nestjs-graphql-dataloader": "patch:nestjs-graphql-dataloader@^0.1.28#./add-nestjs-dataloader-types.diff",
    "nestjs-pino": "^1.2.0",
    "passport": "^0.4.1",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "pino": "^6.7.0",
    "pino-std-serializers": "^2.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "src": "link:src",
    "transliteration": "^2.1.11"
  },
  "devDependencies": {
    "@apollo/gateway": "0.17.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@spotify/polly-jest-presets": "^3.1.1",
    "@types/express": "4.17.8",
    "@types/graphql-relay": "^0.6.0",
    "@types/graphql-upload": "^8.0.4",
    "@types/jest": "26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.8",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "apollo-server-testing": "npm:^2.19.2",
    "eslint": "7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "fast-check": "^2.12.1",
    "husky": "^4.3.0",
    "jest": "26.5.2",
    "lint-staged": "^10.4.0",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "26.4.1",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7",
    "typescript-language-server": "^0.4.0"
  },
  "resolutions": {
    "fs-capacitor": "^6.2.0",
    "graphql-upload": "^11.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext ts"
    ],
    "*.{md,html,json,js,yaml,yml}": [
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "testRegex": "\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "src/app-config/*"
    ],
    "testEnvironment": "node"
  }
}
